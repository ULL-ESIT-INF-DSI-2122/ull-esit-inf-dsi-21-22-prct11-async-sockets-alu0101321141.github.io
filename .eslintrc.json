{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "quotes": "off",
        "require-jsdoc": "off",
        "valid-jsdoc": "off",
        "max-len": "off",
        "object-curly-spacing": "off",
        "space-before-blocks": "off",
        "eslint-disable-next-line" : "new-cap"
    }
}
